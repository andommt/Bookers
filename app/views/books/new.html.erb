<h1>Books</h1>
<% if @list.errors.any? %>
  <%= @list.errors.count %>件のエラーが発生しました
  <% @list.errors.full_messages.each do |message| %>
    <%= message %>
  <% end %>
<% end %>

<!-- form_with部分 -->
<%= form_with model: @list, url:'/booklists', local:true do |f| %>

  <thead>
    <tr>
      <th>Titele</th>
      <th>Body</th>
    </th>
  </thead>

<h2>NewBook</h2>

  <h4>titel</h4>
  <%= f.text_field :title %>
  <h4>body</h4>
  <%= f.text_area :body %>
 
 
<br><%= f.submit 'Create Book' %></br>
  
<% end %>



  

